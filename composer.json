{
  "name": "vresh/twilio-bundle",
  "type": "symfony-bundle",
  "description": "A simple Symfony 2, 3 bundle for the official sdk provided by Twilio.",
  "keywords": [
    "Twilio"
  ],
  "homepage": "https://github.com/fridolin-koch/VreshTwilioBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Fridolin Koch",
      "email": "info@fridokoch.de"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">7.4",
    "symfony/framework-bundle": "^4.4",
    "twilio/sdk": ">=4.0.2 <5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.5",
    "php-coveralls/php-coveralls": "~v2.5"
  },
  "autoload": {
    "psr-4": {
      "Vresh\\TwilioBundle\\": ""
    }
  }
}
